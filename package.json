{
  "name": "tweakpine",
  "version": "0.1.0",
  "description": "Real-time parameter tweaking for AlpineJS & Vanilla JS apps",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "alpinejs": ">=3.0.0",
    "motion": ">=11.0.0"
  },
  "peerDependenciesMeta": {
    "alpinejs": {
      "optional": true
    },
    "motion": {
      "optional": true
    }
  },
  "devDependencies": {
    "alpinejs": "^3.13.0",
    "motion": "^12.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "alpine",
    "alpinejs",
    "vanilla-js",
    "animation",
    "tweaking",
    "parameters",
    "devtools"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tweakpine"
  }
}
